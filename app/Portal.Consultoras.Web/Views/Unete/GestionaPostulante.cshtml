<script>
    var sord = '';
    var sidx = '';
    var urlLoad = '@Url.Content("~/Content/Images/Loading.gif")';

    $(document).ready(function () {

    $('.headerGrid').hide();
    $('.container_landing').css('overflow-y', 'scroll');

        $("#Estado").val(2);

        $("body").on("click", "[data-paginacion]", function (e) {
            e.preventDefault();
            var obj = $(this);
            var accion = obj.attr("data-paginacion");
            if (accion === "back" || accion === "next") {
                CambioPagina(obj);
            }
        });

        $("body").on("change", "[data-paginacion]", function (e) {
            e.preventDefault();
            var obj = $(this);
            var accion = obj.attr("data-paginacion");
            if (accion === "page" || accion === "rows") {
                CambioPagina(obj);
            }
        });


        fnGrilla(null, null, 2);
    });

    function getValueFromDataAttrib(id, col)
    {

       
        var parent =   $('[data-id="' + id + '"]');

        if(  $(parent).length>0)
        {
            var child = $(parent).filter('[data-colname="' + col + '"]'); 

            if ($(child).length == 1)
            {
                return $(child[0]).html();
            }
            else{
                return '';
            }
        }
        else{
            return '';
        }
    }

   function loadExtraData(solicitudPostulanteID)
    {
       
       var option = $('#Estado').val();

       switch (option) {
           case "0":  // Pendiente de confirmar correo.
               $('#extraZonaOrigenDiv').show();
               $('#extraZonaOrigen').html(getValueFromDataAttrib(solicitudPostulanteID, 'ZonaOrigen'));
               $('#extraSeccionOrigenDiv').show();
               $('#extraSeccionOrigen').html(getValueFromDataAttrib(solicitudPostulanteID, 'SeccionOrigen'));
               break;
           case "9":  // Pendiente de confirmar correo.
               $('#extraZonaOrigenDiv').show();               
               $('#extraZonaOrigen').html(getValueFromDataAttrib(solicitudPostulanteID, 'ZonaOrigen'));
               $('#extraSeccionOrigenDiv').show();
               $('#extraSeccionOrigen').html(getValueFromDataAttrib(solicitudPostulanteID, 'SeccionOrigen'));
           break;

       }
   }

    function OrderListByColumnn(element, currentSidx) {
       
        sidx = currentSidx;

        if ($(element).hasClass('desc') == true) {
            sord = 'asc';
        }
        else if ($(element).hasClass('asc') == true) {
            sord = 'desc';
        }
        else {
            sord = 'asc';
        }

        $('.asc').removeClass('asc');
        $('.desc').removeClass('desc');

        $(element).addClass(sord);

        fnGrilla();

    }

    function CambioPagina(obj) {
        var rpt = paginadorAccionGenerico(obj);
        if (rpt.page == undefined) {
            return false;
        }

        var rows = rpt.rows.replace('ver ', '');

        fnGrilla(rpt.page, rows);
        return true;
    }


    function ArmarListaCliente(array) {
        return SetHandlebars("#cliente-template", array);
    }

    function ArmarListaClientePaginador(data) {
        return SetHandlebars("#paginador-template", data);
    }


    function fnGrilla(page, rows, defaultStatus) {       

        $('#divListaCliente').html('<div style="text-align: center;">Cargando Lista de solicitudes<br><img src="' + urlLoad + '" /></div>');

        var obj = {

            model: {
                PrefijoISOPais: $("#PrefijoISOPais").val(),
                FechaDesde: $("#FechaDesde").val(),
                FechaHasta: $("#FechaHasta").val(),
                Nombre: $('#Nombre').val(),
                Estado: defaultStatus == null ? $("#Estado").val() : 2,
                DocumentoIdentidad:  $("#DocumentoIdentidad").val(),
                CodigoZona: $("#CodigoZonaFiltro").val(),
                sidx: sidx,
                sord: sord,
                page: page || 1,
                rows: rows || $($('[data-paginacion="rows"]')[0]).val() || 10
            }
        };

        $.ajax({
            type: 'POST',
            url: '@Url.Action("ConsultarSolicitudesPostulanteV2", "Unete")',
            dataType: 'json',
            contentType: 'application/json; charset=utf-8',
            data: JSON.stringify(obj),
            async: true,
            success: function (data) {

                var html = ArmarListaCliente(data.rows);
                $('#divListaCliente').html(html);
                var htmlPaginador = ArmarListaClientePaginador(data);
                $('#paginadorCab').html(htmlPaginador);
                $('#paginadorPie').html(htmlPaginador);
                $("#paginadorCab [data-paginacion='rows']").val(data.Registros || 10);
                $("#paginadorPie [data-paginacion='rows']").val(data.Registros || 10);

                configureGrid();

            },
            error: function (error) {
               
            }
        });
    }

    function LoadSpinner(elementId)
    {

        $('#'+elementId).html('<div style="text-align: center; color:white;">Cargando, por favor espere<br><img src="' + urlLoad + '" /></div>');
    }

    function hideColumn(colName) {
        $('[data-colname="' + colName + '"]').hide();
    }
    function showColumn(colName) {
        $('[data-colname="' + colName + '"]').show();
    }

    function configureGrid() {

        var option = $('#Estado').val();
      
        if (typeof option != 'undefined' && option != null) {

            switch (option) {
                case "0": // TODOS
                    $('#blackGrid').css('width', '1550px')
                    showColumn('FechaCreacion'); $('[data-colname="FechaCreacion"]').css('width', '65px');
                    showColumn('CampanaRegistro'); $('[data-colname="CampanaRegistro"]').css('width', '65px');
                    showColumn('Tipo'); $('[data-colname="Tipo"]').css('width', '49px');
                    showColumn('FuenteIngreso'); $('[data-colname="FuenteIngreso"]').css('width', '50px');
                    showColumn('NombreCompleto'); $('[data-colname="NombreCompleto"]').css('width', '120px');
                    showColumn('TipoDocumento'); $('[data-colname="TipoDocumento"]').css('width', '45px');
                    showColumn('NumeroDocumento'); $('[data-colname="NumeroDocumento"]').css('width', '100px');
                    hideColumn('Correo'); $('[data-colname="Correo"]').css('width', '120px');
                    showColumn('CodigoZona'); $('[data-colname="CodigoZona"]').css('width', '45px');
                    showColumn('CodigoSeccion'); $('[data-colname="CodigoSeccion"]').css('width', '45px');
                    showColumn('CodigoTerritorio'); $('[data-colname="CodigoTerritorio"]').css('width', '45px');
                    showColumn('Direccion'); $('[data-colname="Direccion"]').css('width', '150px');
                    hideColumn('CodigoConsultora'); $('[data-colname="CodigoConsultora"]').css('width', '150px');
                    hideColumn('FechaIngreso'); $('[data-colname="FechaIngreso"]').css('width', '150px');
                    showColumn('Ubicacion'); $('[data-colname="Ubicacion"]').css('width', '55px');
                    showColumn('EvaluacionCrediticia'); $('[data-colname="EvaluacionCrediticia"]').css('width', '55px');
