<script>

    function setImageSrc(element, image)
    {
        $("#" + element).parent().hide();
        if (image != '') {
            $("#" + element).attr("src", image);
            $("#" + element).parent().show();
        }
    }

    function verAdjunto2(nombreArchivo, tipo) {
        setImageSrc('image01Visor', '');
        generalLoading();
 
        $.post('@Url.Action("ConsultarAdjuntoImageSource", "Unete")', { nombreArchivo: nombreArchivo, tipo: tipo }, function (img) {

            setImageSrc('image01Visor', img);
            generalCloseLoading();
        }).done(function () {
         
            
        });
    }
    function CerrarDiv(opt) {
        switch (opt) {
            case 1:
                $('#divEvaluacionCrediticia').hide();
                break;
            case 2:
                $('#divEditarCliente').hide();
                break;
            case 3:
                $('#divEliminarCliente').hide();
                break;
            case 4:
                $('#divConsultarUbicacion').hide();
                break;
            case 5:
                $('#divEditarDireccion').hide();
                break;
            case 6:
                $('#divEditarManualDireccion').hide();
                break;
            case 7:
                $('#divEditarSolicitud').hide();
                break;
        }

        if (opt == 1) {
            $('#divNotiNombre').hide();
            $('#divNotiCorreo').hide();
        }
        else {
            $('#divNotiNombre2').hide();
            $('#divNotiCorreo2').hide();
        }     
    }

    function changeTab01(ele)
    {        

        if ($(ele).hasClass('purpleBorder') == false)
        {
            $('.purpleBorder').removeClass('purpleBorder');
            $(ele).toggleClass('purpleBorder');

            $('.tabDocContent').hide();
            $('.tab01Content').fadeIn();

            setImageSrc('image01Visor', '');
        }

    }

    function changeTab02(ele) {        

        if ($(ele).hasClass('purpleBorder') == false) {
            $('.purpleBorder').removeClass('purpleBorder');
            $(ele).toggleClass('purpleBorder');

            $('.tabDocContent').hide();
            $('.tab02Content').fadeIn();

            setImageSrc('image02Visor', '');
        }

    }
</script>

