@model Portal.Consultoras.Web.Models.IncentivosModel
@using Portal.Consultoras.Common;
@using System.Configuration;
@{
    ViewBag.Title = "Mis Incentivos";

    string path = ConfigurationManager.AppSettings.Get(Constantes.ConfiguracionManager.BonificacionesURL);
    string token = ViewBag.Token ?? "";
    string _URL_Incentivos = string.Format("{0}/ingresar/{1}", path, token);
}

<object type="text/html"
        style="width:100%;height:1200px;"
        data="@_URL_Incentivos">
</object>