
@model Portal.Consultoras.Web.Models.MisCertificados.MiCertificadoModel

<div style="display:block; width:510px; float:left;">
    <p style="margin-top:0pt; margin-bottom:0pt; text-align:right; line-height:115%; font-size:12pt">
        <span style="width:35.4pt; display:inline-block">&#xa0;</span>
        <span style="width:35.4pt; display:inline-block">&#xa0;</span>
        <span style="width:35.4pt; display:inline-block">&#xa0;</span>
        <span style="width:35.4pt; display:inline-block">&#xa0;</span>
        <span style="width:35.4pt; display:inline-block">&#xa0;</span>
        <span style="width:35.4pt; display:inline-block">&#xa0;</span>
        <span style="width:35.4pt; display:inline-block">&#xa0;</span>
        <span style="width:35.4pt; display:inline-block">&#xa0;</span>
        <span style="width:35.4pt; display:inline-block">&#xa0;</span>
        <span style="font-family:Arial">No. @Model.NumeroSecuencia</span>
    </p>
    @if (Model.Pais == "Peru")
    {
        <p style="margin-top: 0pt; margin-bottom: 0pt; text-align: justify; line-height: 115%; font-size: 12pt;">
            <span style="font-family: Arial;">&nbsp;</span>
        </p>
    }
    else
    {
        <p style="margin-top: 0pt; margin-bottom: 0pt; text-align: justify; line-height: 115%; font-size: 12pt;">
            <span style="font-family: Arial;">@Model.Ciudad,</span>
            <span style="font-family: Arial;">@Model.FechaCreacion</span>
