    $.jgrid.extend({
        Editar: function (ID, CUV, CampaniaID, Flag, CodigoOferta, ISO, FlagCheck, FlagOrden, CodigoSAP, Stock) {
            if (!ID) {
                alert("Debe seleccionar un Producto, verifique");
                return false;
            }
            LimpiarDatos();
            $("#matriz-imagenes-paginacion, #file-upload-matriz").empty();
            var imgSeleccionada = $($("#list").jqGrid('getCell', ID, 'Imagen')).attr('src');
            $("#ImagenProducto").val(formatUrl(imgSeleccionada));

            ofertaLiquidacionObj.limpiarFiltrosNemotecnico();

            ofertaLiquidacionObj.obtenerImagenes(CodigoSAP, 1, true).done(function (data) {
                llenarDatosFormulario(ID, CUV, CampaniaID, Flag, CodigoOferta, ISO, FlagCheck, FlagOrden, CodigoSAP, Stock);

                ofertaLiquidacionObj.actualizarPais(paisID);

                var params = {
                    idMatrizComercial: data.idMatrizComercial,
                    idImagenMatriz: 0,
                    paisID: paisID,
                    codigoSAP: CodigoSAP,
                    descripcionOriginal: jQuery("#list").jqGrid('getCell', ID, 'Descripcion'),
                };
                ofertaLiquidacionObj.crearObjetoUpload(params);
                closeWaitingDialog();
            });

            return false;
        },
        Eliminar: function (ID, CUV, CampaniaID, Flag, CodigoOferta, ISO, FlagCheck, FlagOrden) {
            var elimina = confirm('¿ Está seguro que desea deshabiltar el producto seleccionado?');
            if (!elimina)
                return false;

            var Item = {
                CampaniaID: CampaniaID,
                CUV: CUV,
                PaisID: paisID
            };

            waitingDialog({});
            jQuery.ajax({
                type: 'POST',
                url: baseUrl + 'OfertaLiquidacion/DeshabilitarOfertaWeb',
                dataType: 'json',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(Item),
                async: true,
                success: function (data) {
                    if (checkTimeout(data)) {
                        closeWaitingDialog();
                        LimpiarDatos();
                        if (data.success == true) {
                            alert(data.message);
                            jQuery("#list").setGridParam({ datatype: 'json', page: 1 }).trigger('reloadGrid');
                        }
                        else
                            alert(data.message);
                    }
                },
                error: function (data, error) {
                    if (checkTimeout(data)) {
                        closeWaitingDialog();
                        alert("ERROR");
                    }
                }
            });
            return false;
        },
        Remover: function (ID, CUV, CampaniaID, Flag, CodigoOferta, ISO, FlagCheck, FlagOrden) {
            var elimina = confirm('¿ Está seguro que desea eliminar el producto seleccionado?');
            if (!elimina)
                return false;

            var Item = {
                CampaniaID: CampaniaID,
                CUV: CUV,
                PaisID: paisID
            };

            waitingDialog({});
            jQuery.ajax({
                type: 'POST',
                url: baseUrl + 'OfertaLiquidacion/RemoverOfertaLiquidacion',
                dataType: 'json',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(Item),
                async: true,
                success: function (data) {
                    if (checkTimeout(data)) {
                        closeWaitingDialog();
                        LimpiarDatos();
                        if (data.success == true) {
                            alert(data.message);
                            jQuery("#list").setGridParam({ datatype: 'json', page: 1 }).trigger('reloadGrid');
                        }
                        else
                            alert(data.message);
                    }
                },
                error: function (data, error) {
                    if (checkTimeout(data)) {
                        closeWaitingDialog();
                        alert("ERROR");
                    }
                }
            });
            return false;
        }
    });

