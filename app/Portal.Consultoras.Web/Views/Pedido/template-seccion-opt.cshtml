<div class="fondo_gris">
    <div id="" class="contenedorCupon">
        <div id="" class="contenedorCuponConocelo content_cupon_pedido" style="display: none;">
            Tienes un dscto exclusivo para ti,
            <a id="linkConocerDescuento" class="link_cupon_home"> conócelo aquí</a>
        </div>
        <div id="" class="contenedorCuponInfo content_cupon_pedido" style="display: none;">
            Agrega alguna oferta web para hacer válido tu cupón de descuento de 00 % || S/. 00.00
        </div>
    </div>
    <hr class="clear"/>
    <div id="divContenedorListaEstrategia">
        <div class="contenedor_carousel">
            @if (ViewBag.Source == 1)
            {
                <div class="text-uppercase">
                    <h1>
                        OFERTAS <b>PARA TI @ViewBag.NombreConsultora</b>
                    </h1>
                    <div class="texto_exclusivo_web">
                        EXCLUSIVAS SÓLO POR WEB
                    </div>
                </div>
            }
            else
            {
                <div class="text-uppercase">
                    <h1>
                        OFERTAS <b style="color: #e81c36">@ViewBag.NombrePalanca</b>
                    </h1>
                    <div class="texto_exclusivo_pedido_bpt">
                        @ViewBag.NombreConsultora, PRUEBA LAS VENTAJAS DE COMPRAR OFERTAS PERSONALIZADAS
                    </div>
                </div>
            }
        
            <hr class="clear"/>
            <div class="contenedor_productos">
                <div id="cierreCarousel" class="background_carousel" style="display: none;"></div>
                <div class="contenedor_carousel_productos content_carrusel_agregado slider center" id="divListadoEstrategia">
                </div>
            </div>
            <hr class="clear"/>
        </div>
    </div>
</div>