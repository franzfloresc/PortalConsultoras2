                </div>

                <div style="display: none;">
                    @using (Html.BeginForm("", "", FormMethod.Post, new { id = "frmConsultoraOnline" }))
                        {
                    <input type="hidden" name="lanzarTabConsultoraOnline" value="true">
                        }
                </div>
            </div>
            }
        </div>
