                        <div style="float:right;">
                            <div class="content_BarrMensaje">
                                <div class="mensaje_barra" style="display: inline;"></div>
                                <div class="agrega_barra"></div>
                            </div>
                            <hr class="clear" />
                            <div class="flechita_barra"></div>
                        </div>
                        <hr class="clear" />
                    </div>
                </div>
                <hr class="clear" />
