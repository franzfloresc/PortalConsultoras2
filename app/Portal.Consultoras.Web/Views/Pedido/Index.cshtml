    <input id="hdCuvRecomendado" type="hidden" />
    <input id="hdnHasFlexipago" type="hidden" value="@Model.IndicadorFlexiPago" />
    <input id="hdfEstadoPedido" type="hidden" value="@Model.EstadoPedido" />
