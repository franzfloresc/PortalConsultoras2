                                <select id="ddlClientes" onchange="CambiarCliente(this)"></select>
                            </div>
                            <div class="texto_encabezado_pedidos acciones"></div>
                        </div>

                        <div id="tbobyDetallePedido"></div>
                    </div>
                </div>

                <div class="paginador_pedidos mostrarPaginadorPedidos inferior">
                    <div id="paginadorPie">
                    </div>
                </div>

                <div class="clear"></div>
            </div>

