@using Portal.Consultoras.Common
@model Portal.Consultoras.Web.Models.DetalleEstrategia.DetalleEstrategiaBreadCrumbsModel

@if (Model.Visible)
{
<ul class="breadcumbs">
    @*<li data-opcion="@Model.Inicio.Texto"><a href="@Model.Inicio.Url">@Model.Inicio.Texto</a></li>*@
    <li data-opcion="@Model.Inicio.Texto"><a href="#" onclick="eventBreadCumb('@Model.Inicio.Url', '@Model.Inicio.Texto' )">@Model.Inicio.Texto</a></li>

    <li data-opcion="@Model.Ofertas.Texto"><a href="#" onclick="eventBreadCumb('@Model.Ofertas.Url', '@Model.Ofertas.Texto')">@Model.Ofertas.Texto</a></li>
    @if (!string.IsNullOrWhiteSpace(Model.Palanca.Texto))
    {
        <li data-opcion="@Model.Palanca.Texto"><a href="#" onclick="eventBreadCumb('@Model.Palanca.Url','@Model.Palanca.Texto')">@Model.Palanca.Texto</a></li>
    }
    <li data-opcion="@Model.Producto.Texto"><a href="#" onclick="eventBreadCumb('@Model.Producto.Texto','@Model.Producto.Texto')" id="estrategia-breadcrumb">@Model.Producto.Texto</a></li>
</ul>
}
