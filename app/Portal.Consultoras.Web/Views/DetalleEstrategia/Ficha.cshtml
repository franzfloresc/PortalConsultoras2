@model Portal.Consultoras.Web.Models.DetalleEstrategiaFichaModel
@using Portal.Consultoras.Common

@{
    var vbSimbolo = ViewBag.Simbolo ?? "";
}

@section styles{
    <link href="@Url.Content("~/Content/Css/Site/Ficha/Ficha.css")" rel="stylesheet" />

}

<div class="imagen_fondo_detalle" style="background-image: url('https://S3.amazonaws.com/consultorasPRD/SomosBelcorp/Matriz/PE/PE_Desktop_Fondo_del_ShowRoom_en_el_Contenedor_Ofertas_Venta 50A_2018630811_01_ytmwyupjny.jpg');"></div>
<div class="contenedor_detalle texto_font" data-item="" data-tag-html="@ViewBag.Campania">
    <div class="contenedor_fichas" data-OrigenPedidoWeb="@Model.Origen">
        <div style="display:none;">
            <div id="data-estrategia" data-estrategia="@Json.Encode(Model)"></div>
        </div>
        @if (Model.TieneSession)
        {
            <div class="contenedor_ficha_producto" id="seccion_razor">
                <div class="ficha_producto">
                    <div class="contenedor_imagen">
                        <img class="contenedor_img" src="@Model.FotoProducto01" alt="">
                    </div>
                    <div class="texto_referencial">
                        <span data-texto_referencial="">* Foto Referencial</span>
                    </div>
                    <div class="contenedor_carrusel_tonos">
                        <div id="carrusel_tonos">
                            <div class="contenedor_imagen_carrusel_tonos" data-item="" data-img="">
                                <img data-imagen_producto="" src="https://s3.amazonaws.com/consultorasPRD/SomosBelcorp/Matriz/PE/PE_200091042201768749_wttltqeyse.png" data-img-lazy="1">
                            </div>
                            <div class="contenedor_imagen_carrusel_tonos" data-item="" data-img="">
                                <img data-imagen_producto="" src="https://s3.amazonaws.com/consultorasPRD/SomosBelcorp/Matriz/PE/PE_200091042201768749_wttltqeyse.png" data-img-lazy="1">
                            </div>
                            <div class="contenedor_imagen_carrusel_tonos" data-item="" data-img="">
                                <img data-imagen_producto="" src="https://s3.amazonaws.com/consultorasPRD/SomosBelcorp/Matriz/PE/PE_200091042201768749_wttltqeyse.png" data-img-lazy="1">
                            </div>
                            <div class="contenedor_imagen_carrusel_tonos" data-item="" data-img="">
                                <img data-imagen_producto="" src="https://s3.amazonaws.com/consultorasPRD/SomosBelcorp/Matriz/PE/PE_200091042201768749_wttltqeyse.png" data-img-lazy="1">
                            </div>
                        </div>
                    </div>
                    <div class="ficha_redes_sociales">
                        <div data-compartir-campos="" style="display:none;">
                            <input type="hidden" class="CUV" value="@Model.CUV2" />
                            <input type="hidden" class="rsFBMensaje" value="" />
                            <input type="hidden" class="rsWAMensaje" value="" />
                            <input type="hidden" class="rsFBRutaImagen" value="@Model.FotoProducto01" />
                            <input type="hidden" class="rsWARutaImagen" value="@Model.FotoProducto01" />
                            <input type="hidden" class="MarcaID" value="@Model.MarcaID" />
                            <input type="hidden" class="MarcaNombre" value="@Model.DescripcionMarca" />
                            <input type="hidden" class="Nombre" value="@Model.DescripcionResumen" />
                            <input type="hidden" class="ProductoDescripcion" value="@Model.DescripcionDetalle" />
                            <input type="hidden" class="Volumen" value="" />
                            <input type="hidden" class="Palanca" value="LAN" />
                        </div>
                        <div class="texto_redes_sociales">Comparte este producto con tus clientes</div>
                        <a class="btn_compartir_facebook" href="javascript:;" data-compartir="FB">
                            <img class="contenedor_img" src="/Content/Images/Esika/facebook.png">
                        </a>
                    </div>
                </div>
                <div class="ficha_detalle">
                    <div class="contenedor_ficha_etiquetas">
                        <div class="contenedor_etiqueta_odd">
                            <div id="clock" class="clock_odd"></div>
                        </div>
                        <div class="contenedor_etiquetas">
                            <img src="https://s3.amazonaws.com/uploads.hipchat.com/583104/4840045/2izIcVZzfnNuPBe/%C2%A1NUEVO%21.jpg">
                        </div>
                    </div>
                    <div class="contenedor_nombre_precio">
                        <div class="contenedor_izquierdo">
                            <div class="nombre_marca">
                                <span data-nombre_marca="">@Model.DescripcionMarca</span>
                            </div>
                            <div class="descripcion" data-descripcion="">@Model.DescripcionResumen</div>
                        </div>
                        <div class="contenedor_derecho">
                            <div class="precio_valorizado">Valorizado en <span data-precio_valorizado="">@vbSimbolo @Model.PrecioTachado</span></div>
                            <div class="precio_venta_oferta">Precio para ti <b><span data-precio_venta_oferta="">@vbSimbolo @Model.PrecioVenta</span></b></div>
                            <div class="ganancia">GANA <span data-ganancia="">@vbSimbolo @Model.GananciaString</span></div>
                        </div>
                    </div>
                    <div class="contenedor_nombre_descripcion_producto">
                        <div class="nombre_detalle_producto">@Model.DescripcionMarca</div>
                        <div class="descripcion_detalle_producto_ver_mas">
                            @Model.DescripcionResumen
                            <span class="txt_ver_mas">ver más</span>
                        </div>
                    </div>
                    @if (Model.CodigoVariante == Constantes.TipoEstrategiaSet.IndividualConTonos)
                    {
                        if ((Model.Hermanos.Any()))
                        {
                            <div class="contenedor_lista_tonos">
                                <div class="nombre_detalle_producto_tono">@Model.DescripcionMarca</div>
                                <div class="descripcion_detalle_producto_tono">
                                    @Model.DescripcionResumen
                                    <span class="txt_ver_mas">ver más</span>
                                </div>
                                <div class="selector_lista_tonos" data-selector_tono="">
                                    <div class="elige_tono" data-tono_change="1">
                                        <div class="img_item_tono" data-img_item_tono="">
                                            <img src="~/Content/Images/showroom/no_disponible.png" alt="">
                                        </div>
                                        <div class="txt_item_tono txt_bold" data-txt_item_tono="">ELIGE TU TONO</div>
                                        <div class="contenedor_flecha_tonos">
                                            <div class="dibujar_linea dibujar_flecha_tonos" data-flecha-tonos=""></div>
                                        </div>
                                    </div>
                                    <div class="contenedor_items_tonos" data-visible="0">
                                        @foreach (var Tono in Model.Hermanos)
                                        {
                                            <div class="contenedor_item_tono" data-tono-showroom-change="2" data-tono-cuv="@Tono.Cuv" data-tono-nombre="@Tono.NombreBulk" data-tono-descripcion="@Tono.NombreComercial">
                                                <div class="img_item_tono"><img src="@Tono.ImagenBulk" alt=""></div>
                                                <div class="txt_item_tono">@Tono.NombreBulk</div>
                                                <hr class="clear">
                                            </div>
                                        }
                                    </div>
                                </div>
                                <div class="contenedor_lista_tonos_imagen">
                                    @foreach (var Tono in Model.Hermanos)
                                    {
                                        <a class="contenedor_item_tono_imagen" data-tono_change="2" data-tono-cuv="@Tono.Cuv" data-tono-nombre="@Tono.NombreBulk" data-tono-descripcion="@Tono.NombreComercial">
                                            <div class="img_item_tono"><img src="@Tono.ImagenBulk" alt=""></div>
                                            <hr class="clear">
                                        </a>
                                    }
                                </div>
                            </div>
                        }
                    }
                    else if (Model.CodigoVariante == Constantes.TipoEstrategiaSet.CompuestaFija || Model.CodigoVariante == Constantes.TipoEstrategiaSet.CompuestaVariable)
                    {
                        <div class="contenedor_lista_detalle">

                            @foreach (var Hermano in Model.Hermanos)
                            {
                                if ((Hermano.NombreComercial ?? "") == "" && Hermano.Digitable != 1)
                                {
                                    continue;
                                }

                                <div class="texto_contenido">CONTENIDO:</div>
                                <div class="detalle_producto" data-tono="" data-tono-select="" data-tono-digitable="@Hermano.Digitable">
                                    <div style="display: none;">
                                        <input type="hidden" name="nameMarcaID" id="Estrategia_hd_MarcaID" value="@Hermano.IdMarca" />
                                        <input type="hidden" name="namePrecioCatalogo" id="Estrategia_hd_PrecioCatalogo" value="@Hermano.PrecioCatalogo" />
                                    </div>
                                    <div class="marca_cantidad_descripcion_detalle">
