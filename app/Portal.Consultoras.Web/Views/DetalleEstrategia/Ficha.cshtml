                                        </div>
                                        <div class="cantidad_descripcion_detalle">
                                            <span class="cantidad_detalle_producto" data-cantidad_detalle_producto="">x @Hermano.Cantidad</span>
                                            <span class="descripcion_detalle_producto" data-descripcion_detalle_producto="" @* data-tono-cuv-producto="" data-tono-select-nombrecomercial="" *@>
                                                Hermano.NombreComercial
                                            </span>
                                        </div>
                                    </div>
                                    @if (Hermano.Digitable == 1)
                                    {
                                        <div class="selector_tono" data-selector_tono="">
                                            <div class="elige_tono" data-tono-showroom-change="1">
                                                <div class="img_item_tono" data-img_item_tono="" style="display: none;">
                                                    <img src="~/Content/Images/showroom/no_disponible.png" alt="">
                                                </div>
                                                <div class="txt_item_tono txt_bold" data-txt_item_tono="">ELIGE TU TONO</div>
                                                <div class="contenedor_flecha_tonos">
                                                    <div class="dibujar_linea dibujar_flecha_tonos" data-flecha-tonos=""></div>
                                                </div>
                                            </div>
                                            <div class="contenedor_items_tonos" data-visible="0" style="display: none;">
                                                @foreach (var Tono in Hermano.Hermanos)
                                                {
                                                    <div class="contenedor_item_tono" data-tono-showroom-change="2" data-tono-cuv="@Tono.Cuv" data-tono-nombre="@Tono.NombreBulk" data-tono-descripcion="@Tono.NombreComercial">
                                                        <div class="img_item_tono"><img src="@Tono.ImagenBulk" alt=""></div>
                                                        <div class="txt_item_tono">@Tono.NombreBulk</div>
                                                        <hr class="clear">
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                    }
                                    <hr class="clear">
                                </div>
                            }
                        </div>
                    }


                    <div class="contenedor_agregar">
                        <div class="contenedor_rangos" data-cantidad-contenedor="">
                            <input class="txt_cantidad_pedido ValidaNumeral ValidaPasteNumeral" type="text" value="1" size="2" maxlength="2" data-input="cantidad" data-cantidad-input="-">
                            <div>
                                <a class="txt_menos" data-cantidad-agregar="-" data-bloqueada="" onclick="javascript:EstrategiaAgregarModule.DisminuirCantidad(event)">
                                    <div class="dibujar_linea dibujar_menos"></div>
                                </a>
                                <a class="txt_mas" data-cantidad-agregar="+" data-bloqueada="" onclick="javascript:EstrategiaAgregarModule.AdicionarCantidad(event)">
                                    <div class="dibujar_linea dibujar_mas"></div>
                                </a>
                            </div>
                        </div>
                        <a id="btnAgregalo" class="btn_agregalo @Model.ClaseBloqueada" data-bloqueada="@Model.ClaseBloqueada" onclick="javascript:EstrategiaAgregarModule.EstrategiaAgregar(event, true); ">AGRÉGALO</a>
                        <div class="contenedor_agregado">
                            <div class="txt_agregado" data-txt_agregado="">
                                <span class="span_agregado">AGREGADO</span>
                                <div class="check_agregado">
                                    <div class="dibujar_linea dibujar_check_agregado"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="contenedor_limitacion">
                        <div class="signo_exclamacion">
                            <div class="dibujar_signo_exclamacion"></div>
                        </div>
                        <span class="txt_limitacion">Limitado a 5 packs por consultora</span>
                    </div>
                </div>
            </div>
        }
