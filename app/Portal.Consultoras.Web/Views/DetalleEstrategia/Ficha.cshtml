                            <div class="precio_venta_oferta">Precio para ti <b><span data-precio_venta_oferta="">{{SimboloMoneda}} {{PrecioVenta}}</span></b></div>
                            <div class="ganancia">GANA <span data-ganancia="">{{SimboloMoneda}} {{GananciaString}}</span></div>
                        </div>
                    </div>

                    {{#iff CodigoVariante '==' '2001'}}

                    {{#each this.Hermanos as |componente|}}
                    <div class="contenedor_lista_tonos">
                        <div class="nombre_detalle_producto_tono">{{componente.DescripcionMarca}}</div>
                        <div class="descripcion_detalle_producto_tono">
                            {{componente.DescripcionResumen}}
