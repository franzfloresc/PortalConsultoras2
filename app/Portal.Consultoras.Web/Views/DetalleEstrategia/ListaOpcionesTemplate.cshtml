<script id="lista-opciones-template" type="text/x-handlebars-template">
    {{#each this.Hermanos as |componente|}}
    <ul class="selecion_item list-inline">
        <li class="seleccion_img"><img src="{{componente.ImagenBulk}}" alt=""></li>
        <li class="seleccion_title">{{componente.NombreBulk}}</li>
        <li class="seleccion_btn seleccion_cantidad" style="display:none;">
            <div class="contenedor_rangos" id="idcontenedor_rangos" data-cantidad-contenedor="">
                <input class="txt_cantidad_pedido ValidaNumeral ValidaPasteNumeral" type="text" value="1" size="2" maxlength="2" data-input="cantidad" data-cantidad-input="-">
                <div>
                    <a class="txt_menos" data-cantidad-agregar="-" onclick="javascript:EstrategiaAgregarModule.DisminuirCantidad(event)">
                        <div class="dibujar_linea dibujar_menos"></div>
                    </a>
                    <a class="txt_mas" data-cantidad-agregar="+" onclick="javascript:EstrategiaAgregarModule.AdicionarCantidad(event)">
                        <div class="dibujar_linea dibujar_mas"></div>
                    </a>
                </div>
            </div>
        </li>
    </ul>
    {{/each}}
</script>
