<script id="matriz-comercial-template" type="text/x-handlebars-template">
    <input id="hdFlagTransaccion" type="hidden" value="0" />
    <div class="Content_modal">
        <div class="div-3">
            <div class="titF">País:</div>
            <div class="selectP2 borde_redondeado">
                <input type="text" id="txtPaisModal" value="{{pais}}" readonly="readonly" />
            </div>
            <div class="titG">Código SAP:</div>
            <div class="selectP2 borde_redondeado">
                <input type="text" id="txtCodigoSAPModal" value="{{codigoSAP}}" readonly="readonly" />
            </div>
        </div>
        <div class="div-3">
            <div class="titF">Descripción Original:</div>
            <div class="selectP2 borde_redondeado" style="width: 515px;">
                <input type="text" id="txtDescripcionOriginalModal" value="{{descripcionOriginal}}" readonly="readonly" style="width: 515px;" />
            </div>
            <div id="file-upload-add"></div>
        </div>
        <div class="div-3">
            <div id="matriz-comercial-images" class="div-list-img-matriz-comercial">                
            </div>
        </div>
    </div>
</script>
<script id="matriz-comercial-item-template" type="text/x-handlebars-template">
    {{#each imagenes}}
    <div class="titG">
        <div class="selectP2 borde_redondeado aligncenter">
            <img id="img-matriz-{{@@index}}" src='{{Foto}}' class="img-matriz-comercial">
        </div>
        <div class="input_search">
            <div id="file-upload-{{@@index}}">
            </div>
        </div>
    </div>
    {{/each}}
</script>
