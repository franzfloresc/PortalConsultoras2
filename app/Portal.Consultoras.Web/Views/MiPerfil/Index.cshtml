                                            onclick="location.href='@Url.Action("ConfirmarCelular", "MiPerfil")'">
                                        Confirmar
                                    </button>
                                    <!-- FIN - Este botón aparece cuando la consultora tiene pendiente confirmar su celular -->
                                </div>
                                <!--INI HD-4358 -->
                                    @{
                                        var itemNotiWsp= Model.UsuarioOpciones.FirstOrDefault(x => x.OpcionesUsuarioId == Constantes.OpcionesUsuario.NotificarWhatsApp);
                                            
                                    }
                                    @if (itemNotiWsp != null)
                                    {
                                        var readOnlyNotiWsp = string.IsNullOrEmpty(Model.Celular);
                                        var checkNotiWsp = !string.IsNullOrEmpty(Model.Celular) && itemNotiWsp.CheckBox;

                                        <div class="w-100 permisos_cuenta_opciones notificaciones__whatsapp__checkbox">
                                            <div class="d-flex permiso_cuenta_opcion">
                                                <div class="permiso_cuenta_opcion_descripcion">
                                                    @itemNotiWsp.Descripcion
                                                </div>
                                                <div class="grupo_form_permiso_cuenta">
                                                    <input class="optCelCheckbox permiso_cuenta_opcion_checkbox" type="checkbox" data-tipoOpcion="@itemNotiWsp.OpcionesUsuarioId" name="@itemNotiWsp.Codigo" id="@itemNotiWsp.Codigo" checked="@checkNotiWsp" disabled="@readOnlyNotiWsp" style="display:none;" />
                                                    <label for="@itemNotiWsp.Codigo" class="permiso_cuenta_opcion_label"></label>
                                                </div>
                                            </div>
                                        </div>

                                    }
                                <!--FIN HD-4358 -->
