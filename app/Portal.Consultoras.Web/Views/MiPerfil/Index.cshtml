@using Portal.Consultoras.Common
@model Portal.Consultoras.Web.Models.MisDatosModel
@{
    ViewBag.Title = "Mi Perfil";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@if (System.Configuration.ConfigurationManager.AppSettings.Get("PaisesEsika").Contains(ViewBag.PaisAnalytics == null ? "" : ViewBag.PaisAnalytics))
{
    <link rel="stylesheet" href="@Url.Content("~/Content/Css/Site/Esika/mi-perfil.css")">
}
else
{
    <link rel="stylesheet" href="@Url.Content("~/Content/Css/Site/Lbel/mi-perfil.css")">
}

@section styles{
    <style type="text/css">
        .asteriscos {
            color: #e81c36;
        }
    </style>
}

<div class="fondo_f9f9f9">
    <a href="@Url.Action("Index", "Bienvenida", new { area = "" })" style="display: none;" id="volverBienvenida">Volver</a>
    <div class="content_belcorp">
        <div class="fondo_negro_lateral"></div>
        <div class="titulo_interiores titulo_misPagos"><span>MI</span> PERFIL</div>
    </div>
    <hr class="clear" />
    <div class="linea_separadora" style="margin-top:-1px"></div>
</div>
<div class="content_belcorp">
    <div class="pestana_lbel pestana_lbel_mi_perfil"></div>
</div>
<section class="datos_usuario_iniciales w-100">
    <div class="wrapper_content_mi_perfil text-center">
        <div class="foto_consultora">
            <a href="@Url.Action("CambiarFotoPerfil", "MiPerfil", new { area = "" })" title="Editar imagen" class="icono_camara multimarca-background-color"></a>
            <div class="contenedor_foto_consultora">
                <img id="imgFotoPerfilCambiar" src="@ViewBag.FotoPerfil" onerror="this.onerror=null; this.src='@Url.Content(Constantes.ConfiguracionManager.DefaultPerfilImage)'" alt="Imagen Consultora" class="consultora_img ImageLoadPhotoUser"/>
            </div>
        </div>
        <div class="datos_consultora_mi_perfil text-left">
            <div class="nom_consultora text-bold text-uppercase">
                @Model.NombreCompleto
            </div>
            <div class="datos_codigo_y_zona">
                <input type="hidden" id="hdn_CorreoMD" value="@Model.EMail" />
                <input type="hidden" id="hdn_CodigoUsuarioMD" value="@Model.CodigoUsuario" />
                <input type="hidden" id="hdn_CodigoUsuarioReal" value="@Model.CodigoUsuarioReal" />
                <input type="hidden" id="hdn_NombreCompletoMD" value="@Model.NombreCompleto" />
                <input type="hidden" id="hdn_NombreArchivoContratoMD" value="@Model.NombreArchivoContrato" />
                <input type="hidden" id="hdn_CaracterMinimo" value="@Model.limiteMinimoTelef" />
                <input type="hidden" id="hdn_CaracterMaximo" value="@Model.limiteMaximoTelef" />
                <input type="hidden" id="hdn_ServicioSMS" value="@Model.ServiceSMS" />
                <input type="hidden" id="hdn_PuedeActualizar" value="@Model.ActualizaDatos" />
                <input type="hidden" id="hdn_enlaceTerminosCondiciones" value="@ViewBag.UrlPdfTerminosyCondiciones" />
                <input type="hidden" id="hdn_iniciaNumero" value="@Model.IniciaNumeroCelular" />
                <input type="hidden" id="hdn_PaisID" value="@Model.PaisID" />
                @{
                    var TieneDireEntrega = Model.TieneDireccionEntrega ? "1" : "0";
                    var TienePermiCuenta = Model.TienePermisosCuenta ? "1" : "0";
                }
                <input type="hidden" id="hdn_TienedireccionEntrega" value="@TieneDireEntrega" />
                <input type="hidden" id="hdn_TienePermisosCuenta" value="@TienePermiCuenta" />
                @*INI HD-3897*@
                <input type="hidden" id="hdn_FlgCheckSMS" value="@Model.FlgCheckSMS" />
                <input type="hidden" id="hdn_FlgCheckEMAIL" value="@Model.FlgCheckEMAIL" />
                @{
                    var EditarDatos = Model.PaisISO != Constantes.CodigosISOPais.Colombia ? "1" : "0";
                }

                <input type="hidden" id="hdn_EditarDatos" value="@EditarDatos" />
                <input type="hidden" id="hdn_PuedeConfirmarAllEmail" value="@Model.PuedeConfirmarAllEmail" />
                <input type="hidden" id="hdn_PuedeConfirmarAllSms" value="@Model.PuedeConfirmarAllSms" />
                @*FIN HD-3897*@
                <div class="datos_codigo">
                    <span class="text-bold">Código : </span>
                    @if (Model.DigitoVerificador != null && ViewBag.PaisesDigitoControl.Contains(Model.PaisISO))
                    {<span class="text-bold">@Model.CodigoUsuario - <span class="text-bold">@Model.DigitoVerificador</span></span>}
                else
                { <span class="text-bold">@Model.CodigoUsuario</span>}
                </div>

                <div class="datos_zona">
                    <span class="text-bold">Zona : </span>
                    <span class="text-bold">@Model.Zona</span>
                </div>
            </div>
            @if (Model.NombreGerenteZonal != "" && Model.IndicadorConsultoraDigital == 0)
            {
                <div class="datos_gerente_zona">
                    <span class="text-bold">Gerente de zona : </span>
                    <span class="text-bold">@Model.NombreGerenteZonal</span>
                </div>
            }
            @*<a href="@Url.Action("CambiarContrasenia", "MiPerfil", new {area = ""})" title="Cambiar contraseña" class="enlace_cambiar_contrasenia_consultora text-bold">
                <span class="icono_cambio_password"></span>
                <span class="enlace_texto text-uppercase">Cambiar contraseña</span>
            </a>*@
        </div>
    </div>
</section>


@*INI EINCA HD-3180 Y HD-3304 15-01-2019*@
