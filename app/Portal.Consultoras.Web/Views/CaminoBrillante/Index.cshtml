@using Portal.Consultoras.Web.Models.CaminoBrillante
@using Portal.Consultoras.Common

@{
    Layout = "~/Views/Shared/_MainLayoutResponsive.cshtml";
    var origenPedido = ViewBag.EsMobile ? Constantes.OrigenPedidoWeb.CaminoBrillanteMobilePedido_Carrusel : Constantes.OrigenPedidoWeb.CaminoBrillanteDesktopPedido_Carrusel;
    ViewBag.origenPedido = origenPedido;
    var nivelSiguiente = ViewBag.NivelSiguiente;
    var decorador = "";
    ViewBag.TieneCarrusel = ((ViewBag.TieneCarrusel == "1") && ViewBag.TieneOfertasEspeciales) ? ViewBag.TieneCarrusel : "0";
}

@section css{
    @System.Web.Optimization.Styles.Render("~/Bundle/Css/CaminoBrillante")
    <style>
        div.content_responsive {
            margin-top: 0px;
        }
    </style>
}

<link href="~/Content/Css/Site/FichaProductoResponsive/ficha-producto-responsive.css" rel="stylesheet" />



<div id="Modalunbord" class="modal " role="dialog">
    <div class="modal-dialog modal-sm">
        <div class="modal-content">
            @*<button type="button" class="close" data-dismiss="modal">&times;</button>*@

