            <div class="div-3">
                <div class="titI">Referencia:</div>
                <div class="selectA borde_redondeado">
                    <input id="txtReferencia" type="text" value="@Model.vchReferencia" disabled="disabled" />
                </div>
            </div>
            <div class="div-3">
                <div class="titI">Barrio:</div>
                <div class="selectA borde_redondeado">
                    <input id="txtMunicipio" type="text" value="@Model.vchBarrio" disabled="disabled" />
