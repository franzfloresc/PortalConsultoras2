@*@model Portal.Consultoras.Web.Models.DetalleSolicitudModel
@{
    ViewBag.Title = "DetalleCreditoAR";
    Layout = "~/Views/Shared/_SACLayout.cshtml";
}
<script type="text/javascript">

    jQuery(document).ready(function () {
        $("#btnRegresar").click(function () {
            location.href = '@Url.Action("SolicitudCredito", "SolicitudCredito")';
        });
    });
</script>
<div class="wrap_cab">
    <div class="filtros">
        <div class="elements">
            <div class="div-2">
                <h1>Formulario de <span>Solicitud de Credito</span></h1>
            </div>
        </div>
    </div>
</div>

<div class="wrap">
    <div class="container clearfix">
        <div class="div-3">
            <div class="titAutoVertical">Datos de Inscripción:</div>
        </div>
        <div class="border-wrapper_reg">
            <div class="div-3">
                <div class="titI">Tipo Contacto:</div>
                <div class="selectA borde_redondeado">
                    @Html.DropDownListFor(model => model.intTipoContacto, new SelectList(Model.listaTipoContactos, "intsecuencialTablaDetalle", "vchDescripcion"), "-- Seleccionar --", new { id = "ddlTipoContacto", disabled = "disabled" })
                </div>
                <div class="titI">Código Territorio:</div>
                <div class="selectA borde_redondeado">
                    @Html.DropDownListFor(model => model.chrCodigoTerritorio, new SelectList(Model.listaTerritorios, "chrCodigo", "vchDescripcion"), "-- Seleccionar --", new { id = "ddlTerritorio", disabled = "disabled" })
                </div>
            </div>
            <div class="div-3">
                <div class="titI line_height">Código Consultora que Recomienda:</div>
                <div class="selectA borde_redondeado">
                    <input id="txtCodConsultoraRefiere" type="text" value="@Model.vchCodigoConsultoraRecomienda" disabled="disabled" />
                </div>
                <div class="titI line_height">Nombre y Apellidos Consultora que Recomienda:</div>
                <div class="selectA borde_redondeado">
                    <input id="txtNomConsultoraRefiere" type="text" value="@Model.vchNombreConsultoraRecomienda" disabled="disabled" />
                </div>
