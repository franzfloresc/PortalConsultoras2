                    <input id="txtCorreoElectronico" type="text" value="@Model.vchCorreoElectronico" disabled="disabled" />
                </div>
                <div class="titAuto">
                    @if (string.IsNullOrEmpty(Model.vchCorreoElectronico))
                    {
                        <input type="checkbox" disabled="disabled" checked="checked" />
                    }
                    else
                    {
                        <input type="checkbox" disabled="disabled" />
                    }
                    <span>No tengo Correo electrónico</span>
                </div>
            </div>
        </div>
        <div class="div-3">
            <div class="titAutoVertical">Referencia Familiar</div>
        </div>
