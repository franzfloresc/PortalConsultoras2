                <div class="selectA borde_redondeado">
                    @Html.DropDownListFor(model => model.intNivelEducativo, new SelectList(Model.listaNivelEducativo, "intsecuencialTablaDetalle", "vchDescripcion"), "-- Seleccionar --", new { id = "ddlNivelEducativo", disabled = "disabled" })
                </div>
