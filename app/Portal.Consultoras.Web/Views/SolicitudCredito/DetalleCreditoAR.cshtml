                    <input id="txtPrimerApellido" type="text" value="@Model.vchApellidoPaterno" disabled="disabled" />
                </div>
                <div class="titI">Apellido Materno:</div>
                <div class="selectA borde_redondeado">
                    <input id="txtSegundoApellido" type="text" value="@Model.vchApellidoMaterno" disabled="disabled" />
                </div>
            </div>
            <div class="div-3">
                <div class="titI">Nombres:</div>
