                <div class="selectA borde_redondeado">
                    <input id="txtNumeroResidencia" type="text" value="@Model.vchNumeroDireccion" disabled="disabled" />
                </div>
                <div class="titI line_height">Departamento:</div>
                <div class="selectA borde_redondeado">
                    <input id="txtDepartamentoResidencia" type="text" value="@Model.vchDepartamento" disabled="disabled" />
