                    <input id="txtNombres" type="text" value="@Model.vchNombre" disabled="disabled" />
                </div>
                <div class="titI">Fecha Nacimiento (DD/MM/AAAA):</div>
                <div class="selectA borde_redondeado">
                    <input id="txtFechaNacimiento" type="text" value="@(Model.datFechaNacimiento == null ? string.Empty : Convert.ToDateTime(Model.datFechaNacimiento).ToString("dd/MM/yyyy"))" disabled="disabled" />
                </div>
            </div>
            <div class="div-3">
                <div class="titI">Documento de Identidad:</div>
                <div class="selectA borde_redondeado">
                    <select id="ddlDocIdentidad" disabled="disabled">
                        <option value="">-- Seleccionar --</option>
                        <option value="0" selected="@(Model.intTipoDocumento == 0 ? "selected" : "")">DNI</option>
                    </select>
                </div>
                <div class="titI">Número Documento Identidad:</div>
                <div class="selectA borde_redondeado">
                    <input id="txtNumeroDocIdentidad" type="text" value="@Model.vchNumeroDocumento" disabled="disabled" />
                </div>
            </div>
            <div class="div-3">
                <div class="titI">Monotributarista:</div>
                <div class="titAuto">
                    <input id="chkMonotributarista" type="checkbox" value="@Model.chrMonotributarista" disabled="disabled" />
                </div>
                <div class="titI">Número de CUIT (XXYYYYYYYYZ):</div>
                <div class="selectA borde_redondeado">
                    <input id="txtNumeroCUIT" type="text" value="@Model.vchNumeroCUIT" disabled="disabled" />
                </div>
            </div>
            <div class="div-3">
