}

<script src="@Url.Content("~/Scripts/fileuploader.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/handlebars.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.paging.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/AdminContenido/ToastHelper.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/AdminContenido/Paginador.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/AdminContenido/MatrizComercialFileUpload.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/AdminContenido/Estrategia.js")" type="text/javascript"></script>

<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<link href="@Url.Content("~/Content/Css/Toast/jquery.toast.min.css")" rel="stylesheet" />

<div class="wrap_cab">
    <div class="filtros2">
        <div class="elements">
            <div class="div-2">
                <h1><span>Configuracion Palanca</span></h1>
            </div>
            
            <div class="container clearfix">
                <div class="border-wrapper">
                    <table id="list"></table>
                    <div id="pager"></div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="wrap_cab">
    <div class="filtros2">
        <div class="elements">
            <div class="div-2">
                <h1><span>Configuracion Ofertas Home</span></h1>
            </div>
            <div class="div-3">
                <div class="input_horizontal">
                    <input type="button" name="btnNuevo" id="btnNuevo" value="Nuevo" onclick="javaScript: NuevoOfertaHome();" />
                </div>
            </div>
            <div class="container clearfix">
                <div class="border-wrapper">
                    <table id="listOfertas"></table>
                    <div id="pagerOfertas"></div>
                </div>

            </div>
            
        </div>
    </div>
</div>

<div id="DialogMantenimientoPalanca" style="display: none;">
    <div class="Content_modal" id="dialog-content-palanca">
    </div>
</div>

<div id="DialogMantenimientoOfertasHome" style="display: none;">
    <div class="Content_modal" id="dialog-content-ofertas-home">
    </div>
</div>

<script type="text/javascript">
    var urlListarCampanias = '@Url.Action("ListarCampanias", "Campania")';
    var rutaImagenEdit = '@Url.Content("~/Content/Images/Edit.png")';
    var rutaImagenDelete = '@Url.Content("~/Content/Images/Delete.png")';
    var rutaFileUpload = '@Url.Action("ImageLanzamientoUpload", "FileUpload")';
    var rutaTemporal = '@Url.Content("~/Content/Temporales/")';
    var urlS3 = '@ViewBag.UrlS3';
</script>

<script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
<script src="~/Scripts/PortalConsultoras/Palanca/AdminPalanca.js"></script>

