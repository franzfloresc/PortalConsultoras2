@model Portal.Consultoras.Web.Models.AdministrarPalancaModel

@{
    ViewBag.Title = "Administrar Palanca";
}

<script src="@Url.Content("~/Scripts/fileuploader.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/handlebars.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.paging.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/AdminContenido/ToastHelper.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/AdminContenido/Paginador.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/AdminContenido/Nemotecnico.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/AdminContenido/MatrizComercialFileUpload.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/AdminContenido/Estrategia.js")" type="text/javascript"></script>
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<link href="@Url.Content("~/Content/Css/Toast/jquery.toast.min.css")" rel="stylesheet" />

<div class="wrap_cab">
    <div class="filtros2">
        <div class="elements">
            <div class="div-2">
                <h1><span>Configuracion Palanca</span></h1>
            </div>
            <div class="div-3">
                <div class="titG">País:</div>
                <div class="selectA borde_redondeado">
                    @Html.DropDownListFor(model => model.PaisID, new SelectList(Model.ListaPaises, "PaisID", "Nombre"), "-- Seleccionar --", new { id = "ddlPais" })
                </div>
                <div class="titG">Palanca:</div>
                <div class="selectA borde_redondeado">
                    @Html.DropDownListFor(model => model.ConfiguracionPaisID, new SelectList(Model.ListaConfiguracionPais, "ConfiguracionPaisID", "Codigo"), "-- Seleccionar --", new { id = "ddlConfiguracionPais" })
                </div>
            </div>
            <div class="div-3">
                <div class="titG">&nbsp;</div>
                <div class="input_horizontal">
                    <input type="button" name="btnModificar" id="btnModificar" value="Modificar" />
                </div>
            </div>
        </div>
    </div>
</div>


<div id="DialogAdministracionPalanca" style="display: none;">
    <div class="Content_modal" id="dialog-content-palanca">
    </div>
</div>


<script type="text/javascript">
    var urlListarCampanias = '@Url.Action("ListarCampanias", "Campania")';
    var urlCrearCupon = '@Url.Action("CrearCupon", "AdministrarCupon")';
    var urlActualizarCupon = '@Url.Action("ActualizarCupon", "AdministrarCupon")';
    var urlCrearCuponConsultora = '@Url.Action("CrearCuponConsultora", "AdministrarCupon")';
    var urlActualizarCuponConsultora = '@Url.Action("ActualizarCuponConsultora", "AdministrarCupon")';
    var urlListarCuponesPorCampania = '@Url.Action("ListarCuponesPorCampania", "AdministrarCupon")';
    var urlListarCuponConsultorasPorCupon = '@Url.Action("ListarCuponConsultorasPorCupon", "AdministrarCupon")';
    var urlCuponConsultoraCargaMasiva = '@Url.Action("CuponConsultoraCargaMasiva", "AdministrarCupon")';
    var urlImagenEdit = '@Url.Content("~/Content/Images/Edit.png")';
    var urlImagenDelete = '@Url.Content("~/Content/Images/Delete.png")';
    var urlImagenDetail = '@Url.Content("~/Content/Images/icono_lupa.png")';
    var urlImagenEnable = '@Url.Content("~/Content/Images/enable.png")';
    var urlImagenDisable = '@Url.Content("~/Content/Images/disable.png")';
</script>

<script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
<script src="~/Scripts/PortalConsultoras/Palanca/AdminPalanca.js"></script>

