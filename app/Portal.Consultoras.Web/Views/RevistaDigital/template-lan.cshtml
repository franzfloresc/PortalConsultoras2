

<script id="lanzamiento-carrusel-template" type="text/x-handlebars-template">
    {{#each this.listaLan as |lan|}}
        <div class="content_item_ept" data-item="{{lan.EstrategiaID}}" data-item-cuv="{{CUV2}}" data-ImgPrevia="{{lan.EstrategiaDetalle.ImgPrevDesktop}}" data-estrategia="{{JSON2string lan}}">
            <div data-item-tag="body" class="lan_content_body">
                <div class="detalle_set_banner" data-item-tag="contenido">
                    {{#iff lan.EstrategiaDetalle.ImgFichaDesktop '!=' ''}}
                    <div>
                        <div class="etiqueta_nuevo_ept">
                            <img src="{{lan.EstrategiaDetalle.ImgFichaDesktop}}" alt="" style="max-height:50px;" />
                        </div>
                    </div>
                    {{/iff}}
                    {{#if ../Mobile}}
                        <div class="content_set_imagen_banner" data-item-tag="foto" onclick="javascript:EstrategiaVerDetalle({{lan.EstrategiaID}});">
                            <img src="{{lan.FotoProducto01}}" alt="" />
                        </div>
                    {{else}}
                        <div class="content_set_imagen_banner" data-item-tag="foto">
                            <img src="{{lan.FotoProducto01}}" alt="" />
                        </div>
                    {{/if}}
                    <div data-item-tag="descripcion">
                        {{#iff lan.DescripcionResumen '!=' ''}}
                            <div class="titulo_set_banner_ept">{{lan.DescripcionResumen}}</div>
                        {{/iff}}
                        <div class="detalle_set_banner_ept">
                            {{lan.DescripcionCortada}}
                        </div>
                    </div>
                    <div class="producto_nombre_descripcion" data-item-tag="precio">
                        <div class="producto_precio" style="margin-bottom: 3px;">
                            {{#iff lan.Precio '>' 0}}
                                <span class="precio-oferta-normal">{{lan.Simbolo}} {{lan.PrecioTachado}}</span>
                            {{/iff}}
                            <span class="precio-oferta-venta"><b>{{lan.Simbolo}} {{lan.PrecioString}}</b></span>
                        </div>
                    </div>
                    <div class="producto_nombre_descripcion" data-item-tag="agregar">
                        {{#iff PuedeAgregar '!=' '0'}}
                        {{#if ../Mobile}}
                            <div class="liquidacion_rango_home">
                                <a class="cantidad_menos_home" tabindex="0"><img src="/Content/Images/Mobile/Esika/menos.png" alt=""></a>
                                <input type="tel" value="1" size="2" maxlength="2" id="txtCantidad" data-input="cantidad" class="rango_cantidad_pedido ValidaNumeral ValidaPasteNumeral" onkeypress="return FuncionesGenerales.ValidarSoloNumeros(event);" tabindex="0">
                                <a class="cantidad_mas_home" tabindex="0"><img src="/Content/Images/Mobile/Esika/mas.png" alt=""></a>
                            </div>
                            <div class="clear"></div>
                        {{else}}
                            <div class="liquidacion_rango_home" data-cantidad-contenedor="">
                                <input type="text" value="1" min="1" max="99" size="2" maxlength="2" class="liquidacion_rango_cantidad_pedido ValidaNumeral ValidaPasteNumeral" oninput="maxLengthCheck(this, 2)" onkeypress=" return FuncionesGenerales.ValidarSoloNumeros(event); " data-input="cantidad" data-cantidad-input="-">
                                <div class="liquidacion_rango_right_pedido">
                                    <a class="mas" data-cantidad-agregar="+" href="javascript:;"><img src="/Content/Images/Esika/mas.png" alt=""></a>
                                    <a class="menos" data-cantidad-agregar="-" href="javascript:;"><img src="/Content/Images/Esika/menos.png" alt=""></a>
                                </div>
                            </div>
                        {{/if}}
                        <a class="boton_Agregalo_home {{lan.ClaseBloqueada}}" data-bloqueada="{{lan.ClaseBloqueada}}" href="javascript:;" data-campaniaid="{{lan.CampaniaID}}"
                           onclick="javascript:CargarProductoDestacado({ EstrategiaID: '{{lan.EstrategiaID}}', TipoEstrategiaID: '{{lan.TipoEstrategiaID}}', Posicion: {{lan.Orden}}, FlagNueva: '{{lan.FlagNueva}}' }, this); ">
                            AGRÉGALO
                        </a>
                        {{/iff}}
                        {{#if ../Mobile}}
                        {{else}}
                        <div class="clear"></div>
                        <div style="padding-top: 9px; height: 15px;">
                            <div class="agregado product-add" style="text-align: right; display: {{#if lan.IsAgregado }}block;{{else}}none;{{/if}}">
                                AGREGADO
                                <div class="icono"></div>
                            </div>
                        </div>
                        {{/if}}
                    </div>

                    {{#if ../Mobile}}
                    <div class="btn_negro" style="text-align: center;">
                        <a data-item-accion="verdetalle" href="javascript:;">
                            VER MÁS DE ESTE PRODUCTO
                            <img class="rd_arrowright" src="/Content/Images/RevistaDigital/rd_arrowright.png">
                        </a>
                    </div>
                    {{else}}
                    <a class="btn_mas_info_set rd_ver_mas" data-item-accion="verdetalle" href="javascript:;">
                        ver más de este producto
                    </a>
                    {{/if}}
                </div>
            </div>
            {{#iff lan.EstrategiaDetalle.ImgFondoDesktop '!=' ''}}
                <div class="imagen_fondo" data-item-tag="fotofondo">
                    <img src="{{lan.EstrategiaDetalle.ImgFondoDesktop}}" alt="" />
                </div>
            {{/iff}}
        </div>
    {{/each}}
</script>
        
