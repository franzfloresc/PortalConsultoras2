                $(this).parent().parent().find("input").each(function () {
                    $(this).val('');
                    $("#chkEstrella").attr('checked', false);
                    $(this).attr('disabled', true);
                });
                $(this).parent().parent().find("select").each(function () {
                    $(this).val('');
                    $(this).attr('disabled', true);
                });
                $(this).attr('disabled', false);
                PaletaColores();
            } else {
                $(this).parent().parent().find("input").each(function () {
                    $(this).val('');
                    $("#chkEstrella").attr('checked', false);
                    $(this).attr('disabled', false);
                });
                $(this).parent().parent().find("select").each(function () {
                    $(this).val('');
                    $(this).attr('disabled', false);
                });
