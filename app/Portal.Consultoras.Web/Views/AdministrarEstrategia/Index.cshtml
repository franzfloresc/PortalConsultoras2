    $.jgrid.extend({
        Editar: function (id) {
            if (id) {
                waitingDialog({});

                $("#hdEstrategiaID").val(id);
                $('#mensajeErrorCUV').val("")
                $('#mensajeErrorCUV2').val("");
                $(".checksPedidosAsociados").html('');

                var params = {
                    EstrategiaID: $("#hdEstrategiaID").val()
                };

                jQuery.ajax({
                    type: 'POST',
                    url: baseUrl + 'AdministrarEstrategia/FiltrarEstrategia',
                    dataType: 'json',
                    contentType: 'application/json; charset=utf-8',
                    data: JSON.stringify(params),
                    async: true,
                    success: function (data) {
                        $("#hdSimbolo").val(data.Simbolo);

                        if (data.Activo == "1") $("#chkHabilitarOferta").attr("checked", true);
                        else $("#chkHabilitarOferta").attr("checked", false);
                        
                        if (data.FlagDescripcion == "1") $("#chkVisible1").attr("checked", true);
                        else $("#chkVisible1").attr("checked", false);
                        
                        if (data.FlagCEP == "1") $("#chkVisible2").attr("checked", true);
                        else $("#chkVisible2").attr("checked", false);
                        
                        if (data.FlagCEP2 == "1") $("#chkVisible3").attr("checked", true);
                        else $("#chkVisible3").attr("checked", false);
                        
                        if (data.FlagTextoLibre == "1") $("#chkVisible4").attr("checked", true);
                        else $("#chkVisible4").attr("checked", false);
                        
                        if (data.FlagCantidad == "1") $("#chkVisible5").attr("checked", true);
                        else $("#chkVisible5").attr("checked", false);
                        
                        if (data.ColorFondo != "") {
                            $("#hdColorFondo").val(data.ColorFondo);
                        } else {
                            $("#hdColorFondo").val("#FFF");
                        }
                        ActivarDesactivarChecks();

                        $("#hdCampania").val($("#ddlCampania").val());
                        $("#hdTipoEstrategiaID").val(data.TipoEstrategiaID);
                        $("#ddlTipoEstrategia").val(data.TipoEstrategiaID);

                        SeleccionarTipoOferta();

                        $("#txtAlcance").val($("#ddlPais option:selected").html());
                        $("#spanCampania").val($("#ddlCampania option:selected").html());
                        $("#spanTipoEstrategia").val($("#ddlTipoEstrategia option:selected").html().trim());
                        $("#hdEstrategiaID").val(data.EstrategiaID);
                        $("#hdTipoEstrategiaID").val(data.TipoEstrategiaID);
                        $("#hdCampania").val(data.CampaniaID);
                        $("#ddlCampaniaFin").val(data.CampaniaIDFin);
                        $("#hdNumeroPedido").val(data.NumeroPedido);

                        // variable para comparar
                        $("#ImagenProducto").val(data.ImagenURL);
                        $("#imagenProductoAnterior01").val(data.ImagenURL);

                        $("#preview1").attr("src", data.FotoProducto01);
                        $("#preview2").attr("src", data.FotoProducto02);
                        $("#preview3").attr("src", data.FotoProducto03);

                        $('#chkImagenProducto01').attr('checked', false);
                        $('#chkImagenProducto02').attr('checked', false);
                        $('#chkImagenProducto03').attr('checked', false);

                        if (data.FotoProducto01 == "") {
                            $("#preview1").attr("src", '@Url.Content("~/Content/Images/prod_grilla_vacio.png")');
                            $("#chkImagenProducto01").attr('disabled', true);
                        } else { $("#chkImagenProducto01").attr('disabled', false); }
                        if (data.FotoProducto02 == "") {
                            $("#preview2").attr("src", '@Url.Content("~/Content/Images/prod_grilla_vacio.png")');
                            $("#chkImagenProducto02").attr('disabled', true);
                        } else { $("#chkImagenProducto02").attr('disabled', false); }
                        if (data.FotoProducto03 == "") {
                            $("#preview3").attr("src", '@Url.Content("~/Content/Images/prod_grilla_vacio.png")');
                            $("#chkImagenProducto03").attr('disabled', true);
                        } else { $("#chkImagenProducto03").attr('disabled', false); }

                        var img01 = data.FotoProducto01.replace(/^.*[\\\/]/, '');
                        if (img01.lastIndexOf("?") >= 0) {
                            img01 = img01.substring(0, img01.lastIndexOf("?"));
                            $('#chkImagenProducto01').val(img01);
                        }
                        $('#chkImagenProducto01').val(img01);

                        var img02 = data.FotoProducto02.replace(/^.*[\\\/]/, '');
                        if (img02.lastIndexOf("?") >= 0) {
                            img02 = img02.substring(0, img02.lastIndexOf("?"));
                            $('#chkImagenProducto02').val(img02);
                        }
                        $('#chkImagenProducto02').val(img02);

                        var img03 = data.FotoProducto03.replace(/^.*[\\\/]/, '');
                        if (img03.lastIndexOf("?") >= 0) {
                            img03 = img03.substring(0, img03.lastIndexOf("?"));
                            $('#chkImagenProducto03').val(img03);
                        }
                        $('#chkImagenProducto03').val(img03);

                        if ($("#chkImagenProducto03").val() == data.ImagenURL)
                            $('#chkImagenProducto03').attr('checked', true);
                        if ($("#chkImagenProducto02").val() == data.ImagenURL)
                            $('#chkImagenProducto02').attr('checked', true);
                        if ($("#chkImagenProducto01").val() == data.ImagenURL)
                            $('#chkImagenProducto01').attr('checked', true);

                        $("#txtLimite").val(data.LimiteVenta);
                        $("#txtDescripcion").val(data.DescripcionCUV2);
                        $("#txtCUV").val(data.CUV1);
                        $("#ddlEtiqueta1").val(data.EtiquetaID);
                        if (data.Precio != "0") {
                            $("#txtPrecio").val(parseFloat(data.Precio).toFixed(2));
                        } else {
                            $("#txtPrecio").val('');
                        }
                        $("#txtCUV2").val(data.CUV2);
                        $("#ddlEtiqueta2").val(data.EtiquetaID2);
                        if (data.Precio2 != "0") {
                            $("#txtPrecio2").val(parseFloat(data.Precio2).toFixed(2));
                        } else {
                            $("#txtPrecio2").val('');
                        }
                        $("#txtTextoLibre").val(data.TextoLibre);
                        $("#txtCantidad").val(data.Cantidad);
                        $("#hdZonas").val(data.Zona);

                        var strZonas = $("#hdZonas").val();
                        if (strZonas != "") {
                            $.jstree._reference($("#arbolRegionZona")).uncheck_all();
                            var strZonasArreglo = strZonas.split(",");
                            for (i = 0; i < strZonasArreglo.length; i++) {
                                $("#arbolRegionZona").jstree("check_node", "#" + strZonasArreglo[i], true);
                            }
                        } else {
                            $.jstree._reference($("#arbolRegionZona")).uncheck_all();
                            $("#chkSeleccionar").attr("checked", false);
                        }

                        /* SB20-312 - INICIO */
                        if ($('#ddlTipoEstrategia').val() == 3009) {
                            $("#txtOrden").val("");
                            $('#div-orden').hide();

                            $('#txt1-estrella').html('Oferta de Último Minuto');
                            $('#txt2-estrella').hide();
                        }
                        else {
                            $("#txtOrden").val(data.Orden);
                            $('#div-orden').show();

                            $('#txt1-estrella').html('Mostrar estrella');
                            $('#txt2-estrella').show();
                        }

                        if ($('#ddlTipoEstrategia').val() == 3009 || $('#ddlTipoEstrategia').val() == 3011) {
                            $("#ddlEtiqueta1").children('option').hide();
                            $("#ddlEtiqueta1").children("option[value='1']").show();

                            $("#ddlEtiqueta2").children('option').hide();
                            $("#ddlEtiqueta2").children("option[value='3']").show();
                        }
                        else {
                            $("#ddlEtiqueta1").children('option').show();
                            $("#ddlEtiqueta2").children('option').show();
                        }

                        /* SB20-312 - FIN */

                        if (data.FlagEstrella == "1") $("#chkEstrella").attr("checked", true);
                        else $("#chkEstrella").attr("checked", false);
                        $(".checksPedidosAsociados").append('<div class="selectP2 borde_redondeado"><input type="text" id="txtPedidoAsociado" value="' + data.NumeroPedido + '" readonly /></div>');
                        closeWaitingDialog();
                        showDialog("DialogAdministracionEstrategia");
                    },
                    error: function (data, error) {
                        alert(data.message);
                        closeWaitingDialog();
                    }
                });
            }
            return false;
        },
        Eliminar: function (id) {
            var elimina = confirm('¿ Está seguro que desea deshabiltar la estrategia seleccionada?');
            if (!elimina)
                return false;

            if (id) {
                waitingDialog({});

                $("#hdEstrategiaID").val(id);

                var params = {
                    EstrategiaID: $("#hdEstrategiaID").val()
                };

                jQuery.ajax({
                    type: 'POST',
                    url: baseUrl + 'AdministrarEstrategia/DeshabilitarEstrategia',
                    dataType: 'json',
                    contentType: 'application/json; charset=utf-8',
                    data: JSON.stringify(params),
                    async: true,
                    success: function (data) {
                        alert(data.message);
                        fnGrilla();
                        closeWaitingDialog();
                    },
                    error: function (data, error) {
                        alert(data.message);
                        closeWaitingDialog();
                    }
                });
            }
            return false;
        }
    });

</script>
