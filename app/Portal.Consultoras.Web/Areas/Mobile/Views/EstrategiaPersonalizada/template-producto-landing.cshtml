<script id="producto-landing-template" type="text/x-handlebars-template">
    {{#each this.lista as |item|}}
    {{#iff EsBanner}}
    <!--Brayan aqui el Banner-->
    <div data-item="15657" data-item-cuv="95316" data-tipoestrategiaimagenmostrar="3" class="revistadigital-landing slick-slide slick-current slick-active" style="width: 375px;" tabindex="-1" role="option" data-slick-index="3" aria-hidden="false">
        <input class="PosicionEstrategia" value="4" tabindex="0" type="hidden">
        <div class="content_item_home_bpt box_producto_mobile_fix">
            <div style="display:none;"><div data-estrategia="{&quot;CampaniaID&quot;:201804,&quot;EstrategiaID&quot;:15657,&quot;CUV2&quot;:&quot;95316&quot;,&quot;TipoEstrategiaImagenMostrar&quot;:3,&quot;ClaseEstrategia&quot;:&quot;revistadigital-landing&quot;,&quot;Posicion&quot;:4,&quot;TieneVerDetalle&quot;:true,&quot;TienePaginaProducto&quot;:false,&quot;TienePaginaProductoMob&quot;:false,&quot;FotoProducto01&quot;:&quot;https://S3.amazonaws.com/consultorasQAS/SomosBelcorp/Matriz/PE/PE_2000879552017721992_lyfamkstmt.png&quot;,&quot;FotoProductoSmall&quot;:&quot;https://S3.amazonaws.com/consultorasQAS/SomosBelcorp/Matriz/PE/PE_2000879552017721992_lyfamkstmt_small.png&quot;,&quot;FotoProductoMedium&quot;:&quot;https://S3.amazonaws.com/consultorasQAS/SomosBelcorp/Matriz/PE/PE_2000879552017721992_lyfamkstmt_medium.png&quot;,&quot;ImagenURL&quot;:&quot;&quot;,&quot;DescripcionMarca&quot;:&quot;Cyzone&quot;,&quot;DescripcionResumen&quot;:&quot;&quot;,&quot;DescripcionCortada&quot;:&quot;Pack x2 Cyzone Girlink Up 50ml&quot;,&quot;DescripcionDetalle&quot;:&quot;&quot;,&quot;DescripcionCompleta&quot;:&quot;Pack x2 Cyzone Girlink Up 50ml&quot;,&quot;Simbolo&quot;:&quot;S/.&quot;,&quot;Precio&quot;:79.8,&quot;Precio2&quot;:50.9,&quot;PrecioTachado&quot;:&quot;79.80&quot;,&quot;PrecioVenta&quot;:&quot;50.90&quot;,&quot;TipoAccionAgregar&quot;:2,&quot;ClaseBloqueada&quot;:&quot;&quot;,&quot;ProductoPerdio&quot;:false,&quot;TipoEstrategiaID&quot;:3009,&quot;FlagNueva&quot;:0,&quot;IsAgregado&quot;:false,&quot;CodigoEstrategia&quot;:&quot;001&quot;,&quot;CodigoVariante&quot;:&quot;2001&quot;,&quot;ArrayContenidoSet&quot;:[],&quot;ListaDescripcionDetalle&quot;:[],&quot;TextoLibre&quot;:&quot;Limitado a 5 unidades por consultora&quot;,&quot;PrecioPublico&quot;:0,&quot;MarcaID&quot;:3,&quot;UrlCompartir&quot;:&quot;http://localhost:35848//Pdto.aspx?id=PE_[valor]&quot;,&quot;Ganancia&quot;:28.9,&quot;GananciaString&quot;:&quot;28.90&quot;,&quot;TipoEstrategiaDetalle&quot;:{&quot;ImgFondoDesktop&quot;:null,&quot;ImgPrevDesktop&quot;:null,&quot;ImgFichaDesktop&quot;:null,&quot;UrlVideoDesktop&quot;:null,&quot;ImgFichaFondoDesktop&quot;:null,&quot;ImgFondoMobile&quot;:null,&quot;ImgFichaMobile&quot;:null,&quot;UrlVideoMobile&quot;:null,&quot;ImgFichaFondoMobile&quot;:null,&quot;ImgHomeDesktop&quot;:null,&quot;ImgHomeMobile&quot;:null},&quot;Hermanos&quot;:[],&quot;EsOfertaIndependiente&quot;:false,&quot;ImagenOfertaIndependiente&quot;:null,&quot;MostrarImgOfertaIndependiente&quot;:false,&quot;PrecioNiveles&quot;:null,&quot;EsBanner&quot;:false,&quot;TextoBanner&quot;:&quot;&quot;}"></div></div>
            <div onclick="javascript:VerDetalleEstrategia(event);">
                <div style="cursor:pointer;">
                    <div class="producto_img_mobile_bpt">
                        <img data-img-lazy="1" data-lazy-seccion-revista-digital="" class="slick-lazyload-error">
                    </div>
                </div>
                <div class="producto_nombre_descripcion_bpt">
                    <div style="cursor:pointer;">
                        <h6>Cyzone</h6>

                        <p class="nombre_producto_bpt" style="cursor:pointer;">
                            <b class="text_descrition_prd_bpt">{{TextoBanner}}</b>
                        </p>

                        <div class="producto_precio_bpt">
                            <span class="producto_precio_normal_bpt" style="text-decoration: line-through; font-size: 12px;">S/. 79.80</span>
                            <span class="producto_precio_oferta_bpt"><b>S/. 50.90</b></span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="producto_nombre_descripcion">
                <div class="liquidacion_rango_home">
                    <a class="cantidad_menos_home" data-bloqueada="" tabindex="0"><img src="/Content/Images/Mobile/Esika/menos.png" alt=""></a>
                    <input value="1" size="2" maxlength="2" id="txtCantidad" data-input="cantidad" class="rango_cantidad_pedido ValidaNumeral ValidaPasteNumeral " onkeypress="return FuncionesGenerales.ValidarSoloNumeros(event);" tabindex="0" type="tel">
                    <a class="cantidad_mas_home" data-bloqueada="" tabindex="0"><img src="/Content/Images/Mobile/Esika/mas.png" alt=""></a>
                </div>
                <div class="clear"></div>
                <a class="boton_Agregalo_home " data-bloqueada="" onclick="javascript:EstrategiaAgregar(event); " tabindex="0">
                    AGRÉGALO
                </a>
                <div class="clear"></div>
                <div style="margin-top:9px; height: 25px;">
                    <div class="agregado product-add" style="text-align:right;display:none;">
                        AGREGADO
                        <div class="icono"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{else}}
    <div data-item="{{EstrategiaID}}" data-item-cuv="{{CUV2}}" data-tipoestrategiaimagenmostrar="{{TipoEstrategiaImagenMostrar}}" class="{{ClaseEstrategia}}">
        <input type="hidden" class="PosicionEstrategia" value="{{Posicion}}" />
        <div class="content_item_home_bpt box_producto_mobile_fix">
            <div style="display:none;"><div data-estrategia="{{JSON2string item}}"></div></div>
            <div {{#iff TieneVerDetalle }} onclick="javascript:VerDetalleEstrategia(event);" {{/iff}}>
                <div {{#iff TieneVerDetalle }} style="cursor:pointer;" {{/iff}}>
                    <div class="producto_img_mobile_bpt">
                        <img data-src="{{FotoProductoSmall}}" data-img-lazy="1" data-lazy-seccion-revista-digital="" />
                    </div>
                    {{#iff EsOfertaIndependiente '==' '1'}}
                    {{#iff MostrarImgOfertaIndependiente '==' '1'}}
                    <div class="etiqueta_producto" data-item-tag="etiqueta">
                        <img src="{{ImagenOfertaIndependiente}}" alt="Etiqueta Producto" />
                    </div>
                    {{/iff}}
                    {{else}}
                    {{#iff ImagenURL '!=' ''}}
                    <div class="etiqueta_producto" data-item-tag="etiqueta">
                        <img data-src="{{ImagenURL}}" alt="Etiqueta Producto" data-img-lazy="1" data-lazy-seccion-revista-digital="" />
                    </div>
                    {{/iff}}
                    {{/iff}}
                </div>
                <div class="producto_nombre_descripcion_bpt">
                    <div {{#iff TieneVerDetalle }} style="cursor:pointer;" {{/iff}}>
                        {{#iff DescripcionMarca '!=' ''}}
                        <h6>{{DescripcionMarca}}</h6>
                        {{/iff}}

                        {{#iff DescripcionResumen '!=' ''}}
                        <p class="nombre_producto_bpt" style="cursor:pointer;">
                            <b class="text_descrition_prd_bpt">{{DescripcionResumen}}</b>
                        </p>
                        {{/iff}}
                        {{#iff DescripcionCortada '!=' ''}}
                        <p class="nombre_producto_bpt" style="cursor:pointer;">
                            <b class="text_descrition_prd_bpt">{{DescripcionCortada}}</b>
                        </p>
                        {{/iff}}

                        <div class="producto_precio_bpt">
                            {{#iff Precio '>' 0}}
                            <span class="producto_precio_normal_bpt" style="text-decoration: line-through; font-size: 12px;">{{Simbolo}} {{PrecioTachado}}</span>
                            {{/iff}}
                            <span class="producto_precio_oferta_bpt"><b>{{Simbolo}} {{PrecioVenta}}</b></span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="producto_nombre_descripcion">
                {{#iff TipoAccionAgregar '==' '1'}}
                <div class="liquidacion_rango_home">
                    <input type="tel" value="1" size="2" maxlength="2" id="txtCantidad" data-input='cantidad' class="rango_cantidad_pedido ValidaNumeral ValidaPasteNumeral" disabled="disabled" style="border: 1px solid #b0b0b0;width:100%!important" onkeypress="return FuncionesGenerales.ValidarSoloNumeros(event);" {{#iff ClaseBloqueada '!=' ''}} disbled="disable" {{/iff}}>
                </div>
                <div class="clear"></div>
                <a class="boton_Agregalo_home {{ClaseBloqueada}}" data-bloqueada="{{ClaseBloqueada}}" onclick="javascript:EstrategiaAgregar(event); ">
                    AGR&Eacute;GALO
                </a>
                {{/iff}}
                {{#iff TipoAccionAgregar '==' '2'}}
                <div class="liquidacion_rango_home">
                    <a class="cantidad_menos_home" data-bloqueada="{{ClaseBloqueada}}"><img src="/Content/Images/Mobile/Esika/menos.png" alt=""></a>
                    <input type="tel" value="1" size="2" maxlength="2" id="txtCantidad" data-input='cantidad' class="rango_cantidad_pedido ValidaNumeral ValidaPasteNumeral {{ClaseBloqueada}}" onkeypress="return FuncionesGenerales.ValidarSoloNumeros(event);" {{#iff ClaseBloqueada '!=' ''}} disbled="disable" {{/iff}}>
                    <a class="cantidad_mas_home" data-bloqueada="{{ClaseBloqueada}}"><img src="/Content/Images/Mobile/Esika/mas.png" alt=""></a>
                </div>
                <div class="clear"></div>
                <a class="boton_Agregalo_home {{ClaseBloqueada}}" data-bloqueada="{{ClaseBloqueada}}" onclick="javascript:EstrategiaAgregar(event); ">
                    AGR&Eacute;GALO
                </a>
                {{/iff}}
                {{#iff TipoAccionAgregar '==' '3'}}
                <div class="texto_elige_tono">
                    ¡AHORA ELIGE TU TONO!
                </div>
                <div class="clear"></div>
                <a class="btn_elige_tono" onclick="javascript:VerDetalleEstrategia(event);" href="javascript:;">
                    ELIGE TU OPCIÓN
                </a>
                {{/iff}}
                <div class="clear"></div>
                <div style="margin-top:9px; height: 25px;">
                    <div class="agregado product-add" style="text-align:right;display:{{#if IsAgregado }}block;{{else}}none;{{/if}}">
                        AGREGADO
                        <div class="icono"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{/iff}}
    {{/each}}
</script>