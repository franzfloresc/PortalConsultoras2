@using Portal.Consultoras.Common
<script id="estrategia-template2" type="text/x-handlebars-template">
    {{#each this.ListaLan as |item|}}
    <div class="wrapper_resumen_mobile" data-item="{{EstrategiaID}}" OrigenPedidoWeb="@Constantes.OrigenPedidoWeb.RevistaDigitalMobileHomeLanzamiento">
        <div class="rd_lanzamiento">
            <div style="display:none;"><div data-estrategia="{{JSON2string item}}"></div></div>
            <div onclick="javascript: VerDetalleEstrategia(event);">
                {{#iff TipoEstrategiaDetalle.ImgHomeMobile '!=' ''}}
                <img class="rd_lanzamiento_producto" src="{{TipoEstrategiaDetalle.ImgHomeMobile}}">
                {{/iff}}
            </div>
            <div>CONOCE {{DescripcionResumen}}</div>
            <div class="btn_black_solid"><a href="javascript: RDRedireccionarDetalle(event)">DESCUBRE ESTA NOVEDAD</a></div>
            <a href="javascript:RedireccionarContenedorComprar(@Constantes.OrigenPedidoWeb.RevistaDigitalMobileHomeLanzamiento, '@Constantes.ConfiguracionPais.Lanzamiento')">
                VER MÁS LANZAMIENTOS 
                <img class="rd_arrowright" src="@Url.Content("~/Content/Images/RevistaDigital/rd_arrowright.png")" />
            </a>
        </div>
    </div>
    <hr class="clear" />
    {{/each}}
</script>