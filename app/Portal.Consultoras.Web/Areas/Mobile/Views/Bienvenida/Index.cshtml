<div class="wrapper_resumen_mobile">
    <div class="content_320">
        <!--ITEM 1-->
        <div class="item1_mobile">
            @{
                string cabezaEscala = null, pieEscala = null;
                if (Model.CodigoISO == Constantes.CodigosISOPais.Peru ||
                    Model.CodigoISO == Constantes.CodigosISOPais.Colombia ||
                    Model.CodigoISO == Constantes.CodigosISOPais.Ecuador ||
                    Model.CodigoISO == Constantes.CodigosISOPais.Bolivia ||
                    Model.CodigoISO == Constantes.CodigosISOPais.Guatemala ||
                    Model.CodigoISO == Constantes.CodigosISOPais.Salvador ||
                    Model.CodigoISO == Constantes.CodigosISOPais.Dominicana ||
                    Model.CodigoISO == Constantes.CodigosISOPais.PuertoRico) 
                {
                    cabezaEscala = "Ganancia estimada total*";
                    pieEscala = "*Ganancia Total = Ahorro estimado proveniente de tus descuentos por compras en catálogo + ahorro estimado proveniente de compras en Guía de Negocio y Otros.";
                }
                else if (Model.CodigoISO == Constantes.CodigosISOPais.Chile)
                {
                    cabezaEscala = "Comisión y ahorro total*";
                    pieEscala = "*Comisión = Ganancia estimada proveniente de tus descuentos por compras en catálogo; <br>Ahorro = Ahorro estimado proveniente de compras de Guía de Negocio y Otros.";
                }
                else if (Model.CodigoISO == Constantes.CodigosISOPais.Mexico || Model.CodigoISO == Constantes.CodigosISOPais.Panama)
                {
                    cabezaEscala = "Ganancia estimada total*";
                    pieEscala = "*Ganancia Total = Ahorro estimado proveniente de tus descuentos por compras en catálogo + ahorro estimado proveniente de compras en Revista y Otros.";
                }
                else if (Model.CodigoISO == Constantes.CodigosISOPais.CostaRica || Model.CodigoISO == Constantes.CodigosISOPais.Venezuela) 
                {
                    cabezaEscala = "Oportunidad de ahorro total";
                    pieEscala = "";
                }
            }
            <div class="icono_item"></div>
            <div class="titulo_item_mobile" style="text-transform: uppercase;">@(cabezaEscala)</div>
            <div class="monto_ganancia_mobile">
                @Model.Simbolo
                <span data-ganancia="">
                    @Util.DecimalToStringFormat(Convert.ToDecimal(Model.MontoAhorroCatalogo + Model.MontoAhorroRevista), Model.CodigoISO)
                </span>
            </div>
            <div class="texto_inferior" style="text-decoration:underline;"><a href="@Url.Action("Index","Pedido")">Pedido ingresado: @Model.Simbolo <span class="js-span-pedidoingresado">@Util.DecimalToStringFormat(Model.MontoPedido, Model.CodigoISO)</span></a></div>

        </div>
        <!-- FIN ITEM 1-->
    </div>
    <div class="linea_separadora_mobile" style="margin-top:16px"></div>
    <div class="content_320">
        <!--ITEM 2-->
        <div class="item1_mobile">
            <div class="icono_item item_2_home"></div>
            <div class="titulo_item_mobile">MI ESTADO DE CUENTA:</div>
            <div class="texto_inferior"> Mi deuda actual es:</div>
            <div class="monto_ganancia_mobile js-div-deuda">
                @Model.Simbolo <span>@Util.DecimalToStringFormat(Model.MontoDeuda, Model.CodigoISO)</span>
            </div>
            <div class="texto_inferior js-div-vencimiento">Vencimiento: <span>@Model.FechaVencimiento</span></div>
            @if (Model.PaisID == 3) //Chile
            {
                <a title="Paga en línea" class="btn_paga_online_mobile" href="javascript:;" onclick="RedirectPagaEnLineaAnalytics();"> PAGA EN L&Iacute;NEA</a>
            }
            else if (Model.PaisID == 4) //Colombia
            {
                <a title="Paga en línea" class="btn_paga_online_mobile" href="https://www.zonapagos.com/t_belstar/pagos.asp" target="_blank"> PAGA EN L&Iacute;NEA</a>
            }
            else if (Model.PaisID == 12) //Puerto Rico
            {
                <a title="Paga en línea" class="btn_paga_online_mobile" href="@Url.Action("Index", "Paypal", new { area = "Mobile" })"> PAGA EN L&Iacute;NEA</a>
            }
            else if (Model.PaisID == 6 || Model.PaisID == 11)
            {
                @*<a class="boton_facturacion_e btn_100_facturacion" href="@Url.Action("Index", "FacturaElectronica")" target="_blank"> FACTURACI&Oacute;N ELECTR&Oacute;NICA</a>*@
            }
        </div>
        <!--FIN ITEM 2-->
    </div>
    <div class="linea_separadora_mobile" style="margin-top:16px"></div>
</div>
