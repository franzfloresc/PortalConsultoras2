{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jquery": true,
    "prototypejs": true,
    "es6": false
  },
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "script",
    "ecmaFeatures": {}
  },
  "plugins": ["compat"],
  "rules": {
    "compat/compat": 2,
    "no-undef": 0,
    "no-unused-vars": [0, { "vars": "all", "args": "after-used", "ignoreRestSiblings": false, "caughtErrors": "all" }],
    "no-compare-neg-zero": 0,
    "no-console": 0,
    "no-global-assign": 0,
    "no-dupe-keys": 0,
    "no-empty": 0,
    "no-extra-semi": 0,
    "no-extra-boolean-cast": 0,
    "no-irregular-whitespace": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-redeclare": 0,
    "no-unreachable": 0,
    "no-unused-labels": 0,
    "no-useless-escape": 0,
    "use-isnan": 0
  }
}